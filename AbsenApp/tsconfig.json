{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "jsx": "react-native",
    "module": "esnext",
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["app/**/*.tsx", "app/**/*.ts"],
  "exclude": ["node_modules"]
}
